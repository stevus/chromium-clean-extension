{
  "name": "Chrome Cleaner",
  "description": "Clean up Chrome so I can use the Internet.",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Clean Chrome"
  },
  "manifest_version": 2
}
